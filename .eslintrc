{
  "extends": ["eslint:recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "standard/no-callback-literal": "off", // Disable this as we have too many callbacks relying on literals
    "no-throw-literal": "off",
    "camelcase": "off"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "SFJS": true,
    "__VERSION__": true,
    "zip": true
  }
}
